//1.null 병함 연산자
//존재하는 값을 추려내는 기능
//null이나 undefined를 제외한 값을 찾아내는 연산자
let var1;
let var2 = 10;
let var3 = 20;

let var4 = var1 ?? var2; //둘중에 null, undefined아닌값 할당
let var5 = var1 ?? var3;
let var6 = var2 ?? var3; //둘다 아니면 맨처음값 할당

let userName = "aaa";
let userNickName = "nickname"
let displayname = userName ?? userNickName;
//이런식으로 이름이 없으면 닉네임이 나오게 할 수 있음


//2. typeof 연산자
//값의 타입을 문자열로 반환하는 기능을 하는 연산자
let t1 = "hello";
let t1Type = typeof t1; //t1의 타입을 반환


//3. 3항 연산자
// 항을 3개 사용하는 연산자
// 조건에 따라서 참 , 거짓일때 값을 다르게 반환
let var1 = 10;
let res = var1 % 2 === 0 ? "짝수" : "홀수";
